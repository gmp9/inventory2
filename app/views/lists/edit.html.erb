<h1>Editing list</h1>
 
<%= form_for :list, url: list_path(@list), method: :patch do |f| %>
 
  
  <p>
    <%= f.label :item %><br>
    <%= f.text_field :item %>
  </p>
 
  <p>
    <%= f.label :quantity %><br>
    <%= f.text_field :quantity %>
  </p>
  
  <p>
    <%= f.label :date %><br>
    <%= f.text_field :date %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
 
<% end %>
 
<%= link_to 'Back', lists_path %>